{
    "extends": ["./.eslintrc-base.json", "plugin:@typescript-eslint/eslint-recommended"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["error", {"vars": "all", "args": "none"}]
    }
}
